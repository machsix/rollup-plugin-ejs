  "main": "dist/rollup-plugin-ejs.js",
  "jsnext:main": "dist/rollup-plugin-ejs.mjs",
  "module": "dist/rollup-plugin-ejs.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trofima/rollup-plugin-ejs.git"
  },
